include: "XTerm"


Xft.antialias:			true

xcalendar*firstDay:		1
xcalendar*markHoliday:		false
XClock*update:1
XClock*analog: false
XClock*foreground: 		#e1e1e1
XClock*background:		#303030

!-----------------------------------------------
!
!	X T E R M - Setting
xterm.*.loginShell: true

*VT100*loginShell: true
*VT100*colorMode: on
*VT100*boldColors: on
*VT100*dynamicColors: on
*VT100*background:       #202427
*VT100*foreground:       #e1e1e1
*VT100*saveLines: 10000

*VT100*underLine:		off
*VT100*colorBD:			lightyellow
*VT100*colorBDMode:		on
*VT100*colorUL:			yellow
*VT100*colorULMode:		on
*VT100*highlightSelection:	true
*VT100*color0:		#202427
*VT100*color8:	        #757575
!red
*VT100*color1:	        #EA6868
*VT100*color9:	        #FF7272
!green
